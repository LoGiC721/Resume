<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <link rel="icon" type="image/png" href="public/icons/favicon.ico" />
  <title>Resume/CV Design</title>
  <link rel="stylesheet" href="css/resume2.css">
  <script src="https://kit.fontawesome.com/b99e675b6e.js"></script>
</head>

<body>

<div class="resume">
   <div class="resume_left">
     <%if(found[0].img!==null){%>
     <div class="resume_profile">
      <img src="./uploads/<%=found[0]._id%>/<%=found[0].img%>" alt="profile_pic">
     </div>
     <%}%>
     <div class="resume_content">
       <div class="resume_item resume_info">

        <%if((found[0].firstname!==null&&found[0].firstname.length!==0)||
        (found[0].currentposition!==null&&found[0].currentposition.length!==0)||
        (found[0].profile!==null&&found[0].profile.length!==0)){%>
         <div class="title data">
          <%if(found[0].firstname!==null&&found[0].firstname.length!==0){%>
           <p class="bold"><%=found[0].firstname%></p>
           <%}%>
           <%if(found[0].currentposition!==null&&found[0].currentposition.length!==0){%>
           <p class="regular data"><%=found[0].currentposition%></p>
           <br />
           <%}%>

           <%if(found[0].profile!==null&&found[0].profile.length!==0){%>
           <p class="regular data"><%=found[0].profile%></p>
           <%}%>
         </div>
         <hr style="border:solid 1px #B1EAFF;background-color: #B1EAFF;">
        <br>
         <%}%>


         <%if((found[0].address!==null&&found[0].address.length!==0)||
                (found[0].country!==null&&found[0].country.length!==0)||
                (found[0].state!==null&&found[0].state.length!==0)|| (found[0].city!==null&&found[0].city.length!==0)||
                (found[0].pno!==null&&found[0].pno.length!==0)|| (found[0].email!==null&&found[0].email.length!==0)||
                (found[0].pin!==null&&found[0].pin.length!==0)) {%>
       
         <ul>
          
          <%if((found[0].address!==null&&found[0].address.length!==0)||
                (found[0].country!==null&&found[0].country.length!==0)||
                (found[0].state!==null&&found[0].state.length!==0)|| 
                (found[0].city!==null&&found[0].city.length!==0)) {%>


          <li>
             <div class="icon">
               <i class="fas fa-map-signs"></i>
             </div>
             <div class="data">
              <%if(found[0].address!==null&&found[0].address.length!==0){%>
              <%=found[0].address%>
              ,
               <%}%>

               <%if(found[0].country!==null&&found[0].country.length!==0){%>
               <%=found[0].country%>
              ,
               <%}%>

               <%if(found[0].state!==null&&found[0].state.length!==0){%>
               <%=found[0].state%>
               ,
               <%}%>

               <%if(found[0].city!==null&&found[0].city.length!==0){%>
               <%=found[0].city%>
              ,
               <%}%>

               <%if(found[0].pin!==null&&found[0].pin.length!==0){%>
               <%=found[0].pin%>
               <%}%>
             </div>
           </li>
          <%}%>

          <%if(found[0].pno!==null){%>
           <li>
             <div class="icon">
               <i class="fas fa-mobile-alt"></i>
             </div>
             <div class="data">
              <%=found[0].pno%>
             </div>
           </li>
          <%}%>

          <%if(found[0].email!==null&&found[0].email.length!==0){%>
           <li>
             <div class="icon">
               <i class="fas fa-envelope"></i>
             </div>
             <div class="data">
              <%=found[0].email%>
             </div>
           </li>
           <%}%>

         </ul>
         <%}%>
       </div>


       <%if(found[0].skills.length!==0){%>
          <%if(found[0].skills[0].skillsname.length!==0){%>
            <%if((found[0].skills[0].skillsname[0].length!==0||found[0].skills[0].skillsdetails[0]!==null)){%>
       <div class="resume_item resume_skills">
         <div class="title">
           <p class="bold">skill's</p>
         </div>
         <ul>
          
          <%for(let i=0;i<found[0].skills[0].skillsname.length;i++){%>
           <li>
            <%if(found[0].skills[0].skillsname[i].length!==0){%>
             <div class="skill_name data">
              <%=found[0].skills[0].skillsname[i]%>
             </div>
             <%}%>
             <%if(found[0].skills[0].skillsdetails[i]!==null){%>
             <div class="skill_progress">
               <span style="width: <%=found[0].skills[0].skillsdetails[i]%>%;"></span>
             </div>
             <div class="skill_per data"><%=found[0].skills[0].skillsdetails[i]%>%</div>
             <%}%>
           </li>
           <%}%>
          
          
          
         </ul>
       </div>
       <%}%>
       <%}%>
        <%}%>
        <%if((found[0].github!==null&&found[0].github.length!=0)||
                (found[0].linkedin!==null&&found[0].linkedin.length!==0)||
                (found[0].twitter!==null&&found[0].twitter.length!==0)||
                (found[0].website!==null&&found[0].website.length!==0)){%>

       <div class="resume_item resume_social">
         <div class="title">
           <p class="bold">Social</p>
         </div>
         <ul>
          <%if(found[0].twitter!==null&&found[0].twitter.length!==0){%>
           <li>
             <div class="icon">
               <i class="fab fa-twitter-square"></i>
             </div>
             <div class="data">
               <p class="semi-bold">Twitter</p>
               <p><%=found[0].twitter%></p>
             </div>
           </li>
          <%}%>
          <%if(found[0].github!==null&&found[0].github.length!==0){%>
           <li>
             <div class="icon">
               <i class="fab fa-github"></i>
             </div>
             <div class="data">
               <p class="semi-bold">Github</p>
               <p><%=found[0].github%></p>
             </div>
           </li>
          <%}%>
          <%if(found[0].linkedin!==null&&found[0].linkedin.length!==0){%>
           <li>
             <div class="icon">
               <i class="fab fa-linkedin"></i>
             </div>
             <div class="data">
               <p class="semi-bold">Linkedin</p>
               <p><%=found[0].linkedin%></p>
             </div>
           </li>
           <%}%>
           <%if(found[0].website!==null&&found[0].website.length!==0){%>
           <li>
             <div class="icon">
               <i class="fab fa-weebly"></i>
             </div>
             <div class="data">
               <p class="semi-bold">Website</p>
               <p><%=found[0].website%></p>
             </div>
           </li>
           <%}%>
         </ul>

       </div>
 <%}%>
 <%if(found[0].extra.length!==0){%>
          <%if(found[0].extra[0].language.length!==0){%>
            <%if(found[0].extra[0].language[0].length!==0){%>
       <div class="resume_item resume_skills">
         <div class="title">
           <p class="bold">Languages</p>
         </div>
         <ul>
          
          <%for(let i=0;i<found[0].extra[0].language.length;i++){%>
           <li>
             <div class="data">
             <p class="semi-bold data"> <%=found[0].extra[0].language[i]%></p>
             </div>
           </li>
           <%}%>   
         </ul>
       </div>
       <%}%>
        <%}%>
        <%}%>
 
     </div>
  </div>
  <div class="resume_right">
     <%if(found[0].project.length!==0){%>
          <%if(found[0].project[0].projectname.length!==0){%>
            <%if((found[0].project[0].projectname[0].length!==0||
            found[0].project[0].project1description[0].length!==0||
            found[0].project[0].link[0].length!==0||
            found[0].project[0].toolsused[0].length!==0)){%>
    <div class="resume_item resume_education">
        <div class="title">
           <p class="bold">Projects</p>
         </div>
         <ul>
             <%for(let i=0;i<found[0].project[0].projectname.length;i++){%>
         <li>
            <p class="heading data"><%=found[0].project[0].projectname[i]%></p> 
            <br/>

              <%if(found[0].project[0].link[0].length!==0){%>
              <div class="data">
                <p> <i class="fab fa-weebly"></i> <%=found[0].project[0].link[i]%></p>
                 </div>
              <%}%>
              <br />
            <p class="data"><%=found[0].project[0].toolsused[i]%></p> 
            <br />
            <p class="data"><%=found[0].project[0].project1description[i]%></p>
            <br />
            
         </li>
            <%}%> 
      </ul>
      
    </div>
    <%}%>
    <%}%>
    <%}%>
    
    <%if(found[0].work.length!==0){%>
          <%if(found[0].work[0].companyname.length!==0){%>
            <%if((found[0].work[0].companyname[0].length!==0||found[0].work[0].startdate[0]!==null
            ||found[0].work[0].enddate[0]!==null||
            found[0].work[0].jobdescription[0].length!==0||
            found[0].work[0].jobtitle[0].length!==0||
            found[0].work[0].state[0].length!==0||
            found[0].work[0].city[0].length!==0)){%>
    <div class="resume_item resume_work">
      
        <div class="title">
           <p class="bold">Work Experience</p>
         </div>
        <ul>
          <%for(let i=0;i<found[0].work[0].companyname.length;i++){%>
            <li>
            <%if(found[0].work[0].companyname[i].length!==0){%>
            <div class="heading data"><%=found[0].work[0].companyname[i]%></div>
            <br>
            <%}%>
            <%if(found[0].work[0].startdate[i]!==null){%>
            <span><%=found[0].work[0].startdate[i].getDate()%> /
              <%=found[0].work[0].startdate[i].getMonth()+1%> /
              <%=found[0].work[0].startdate[i].getFullYear()%> 
              </span>
            <%}%>

            <%if(found[0].work[0].enddate[i]!==null){%>
              <span>
              -   
              <%=found[0].work[0].enddate[i].getDate()%> /
              <%=found[0].work[0].enddate[i].getMonth()+1%> /
              <%=found[0].work[0].enddate[i].getFullYear()%>
             </span>
             <br>
            <%}%>


            <%if(found[0].work[0].state[i].length!==0||
          found[0].work[0].city[i].length!==0){%>  
              <p class="data"><%=found[0].work[0].state[i]%><br /><%=found[0].work[0].city[i]%></p>
              <%}%>
           <br />
           <%if(found[0].work[0].jobtitle[i].length!==0||found[0].work[0].jobdescription[i].length!==0){%>
            <div class="data">
              <p style="text-align:left"><%=found[0].work[0].jobtitle[i]%></p>
              <p><%=found[0].work[0].jobdescription[i]%><br /><br /></p>
            </div>
           
            <%}%>
          </li>

           <%}%> 
        </ul>
        
    </div>
<%}%>
<%}%>
<%}%>
<%if(found[0].school.length!==0){%>
          <%if(found[0].school[0].name.length!==0){%>
            <%if((found[0].school[0].name[0].length!==0||
            found[0].school[0].startdate[0]!==null
            ||found[0].school[0].enddate[0]!==null||
            found[0].school[0].degree[0].length!==0||
            found[0].school[0].gpa[0]!==null||
            found[0].school[0].location[0].length!==0)){%>
    <div class="resume_item resume_education">
      
      <div class="title">
           <p class="bold">Education</p>
         </div>
      <ul>
        <%for(let i=0;i<found[0].school[0].name.length;i++){%>


          <li>
            <%if(found[0].school[0].name[i].length!==0){%>
            <p class="heading data"><%=found[0].school[0].name[i]%></p>
            <%}%>
            <%if(found[0].school[0].startdate[i]!==null|| found[0].school[0].enddate[i]!==null){%>
            
            <div><%=found[0].school[0].startdate[i].getDate()%> /
              <%=found[0].school[0].startdate[i].getMonth()+1%> /
              <%=found[0].school[0].startdate[i].getFullYear()%>  -   
              <%=found[0].school[0].enddate[i].getDate()%> /
              <%=found[0].school[0].enddate[i].getMonth()+1%> /
              <%=found[0].school[0].enddate[i].getFullYear()%>
              </div>
            <%}%>
            <%if(found[0].school[0].location[i].length!==0){%>  
              <p class="data"><%=found[0].school[0].location[i]%></p>
              <%}%>
           
           <%if(found[0].school[0].gpa[i]!==null||found[0].school[0].degree[i].length!==0){%>
            <div>
              <p class="data"><%=found[0].school[0].degree[i]%></p><br/>
              <p><%=found[0].school[0].gpa[i]%><br /><br /></p>
            </div>
           
            <%}%>
          </li>



           <%}%> 
           
        </ul>
        
    </div>
   <%}%> 
<%}%>
<%}%>
 




<%if(found[0].awards.length!==0){%>
          <%if(found[0].awards[0].awardname.length!==0){%>
            <%if((found[0].awards[0].awardname[0].length!==0||
            found[0].awards[0].awarddate[0]!==null||
            found[0].awards[0].awarder[0].length!==0||
            found[0].awards[0].Awarddescription[0].length!==0)){%>
    <div class="resume_item resume_education">
      
      <div class="title">
           <p class="bold">Awards</p>
         </div>
      <ul>
        <%for(let i=0;i<found[0].awards[0].awardname.length;i++){%>
           
         
          <li>
            <%if(found[0].awards[0].awardname[i].length!==0){%>
              
            <p class="heading data"><%=found[0].awards[0].awardname[i]%></p>
         
            <%}%>
            <%if(found[0].awards[0].awarddate[i]!==null){%>
            <div><%=found[0].awards[0].awarddate[i].getDate()%> /
              <%=found[0].awards[0].awarddate[i].getMonth()+1%> /
              <%=found[0].awards[0].awarddate[i].getFullYear()%>
              
              </div>
            <%}%>

            <%if(found[0].awards[0].awarder[i].length!==0){%>  
              <p class="data"><%=found[0].awards[0].awarder[i]%></p>
              <%}%>
            <br />
              <%if(found[0].awards[0].Awarddescription[i]!==null){%>
            
            <p class="data"><%=found[0].awards[0].Awarddescription[i]%><br /><br /></p>
           <%}%>
          </li>


           <%}%> 
           
        </ul>
        
    </div>
   <%}%> 
<%}%>
<%}%>



 

 <%if(found[0].extra.length!==0){%>
          <%if(found[0].extra[0].strength.length!==0){%>
            <%if(found[0].extra[0].strength[0].length!==0){%>
       <div class="resume_item resume_education">
         <div class="title">
           <p class="bold">Strengths</p>
         </div>
         <ul>
          
          <%for(let i=0;i<found[0].extra[0].strength.length;i++){%>
           <li>
             <div class="info data">
             <p class="semi-bold"> <%=found[0].extra[0].strength[i]%></p>
             </div>
           </li>
           <%}%>   
         </ul>
       </div>
       <%}%>
        <%}%>
        <%}%>
  

        <%if(found[0].extra.length!==0){%>
          <%if(found[0].extra[0].hobbie.length!==0){%>
            <%if(found[0].extra[0].hobbie[0].length!==0){%>
       <div class="resume_item resume_education">
         <div class="title">
           <p class="bold">Hobbies</p>
         </div>
         <ul>
          
          <%for(let i=0;i<found[0].extra[0].hobbie.length;i++){%>
           <li>
             <div class="info data">
             <p class="semi-bold"> <%=found[0].extra[0].hobbie[i]%></p>
             </div>
           </li>
           <%}%>   
         </ul>
       </div>
       <%}%>
        <%}%>
        <%}%>

        <%if(found[0].extra.length!==0){%>
          <%if(found[0].extra[0].goals.length!==0){%>
            <%if(found[0].extra[0].goals[0].length!==0){%>
       <div class="resume_item resume_education">
         <div class="title">
           <p class="bold">Goals</p>
         </div>
         <ul>
          
          <%for(let i=0;i<found[0].extra[0].goals.length;i++){%>
           <li>
             <div class="info data">
             <p class="semi-bold"> <%=found[0].extra[0].goals[i]%></p>
             </div>
           </li>
           <%}%>   
         </ul>
       </div>
       <%}%>
        <%}%>
        <%}%>


  
  </div>
</div>



</div>  

</div>
</body>
</html>